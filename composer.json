{
    "name": "soliantconsulting/soliant-payment",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "wikimedia/composer-merge-plugin": "^1.3",
        "php": "^5.6 || ^7.0",
        "authorizenet/authorizenet": "1.8.8",
        "jms/serializer": "xsd2php-dev as 0.18.0",
        "zendframework/zend-modulemanager": "^2.5",
        "zendframework/zend-servicemanager": "^2.5",
        "zendframework/zend-stdlib": "^2.5",
        "zendframework/zend-http": "^2.5"
    },
    "require-dev": {
        "goetas/xsd2php": "2.*@dev",
        "goetas/xsd-reader": "2.*@dev",
        "squizlabs/php_codesniffer": "^2.3",
        "zendframework/zend-debug": "^2.5"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/goetas/serializer.git"
        }
    ],
    "extra": {
        "merge-plugin": {
            "include": [
                "composer.local.json"
            ],
            "require": [
                "module/PaymentBase/composer.json",
                "module/AuthnetPayment/composer.json"
            ],
            "recurse": true,
            "replace": false,
            "merge-dev": true,
            "merge-extra": false
        }
    },
    "autoload": {
        "psr-4": {
            "Soliant\\AuthnetPayment\\": "module/AuthnetPayment/src/",
            "Soliant\\PaymentBase\\": "module/AuthnetPayment/src"
        }
    }
}